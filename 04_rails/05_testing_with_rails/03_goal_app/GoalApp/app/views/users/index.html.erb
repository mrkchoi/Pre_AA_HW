<h2>All users</h2>
<div class="ui divider"></div>

<div class="ui items">

  <% @users.order('users.username ASC').each do |user| %>
    <div class="item">
      <div class="content">
        <div class="header">
          <%= link_to user.username, user_url(user.id) %>
        </div>
      </div>
    </div>
    <div class="ui divider"></div>
  <% end %>
</div>