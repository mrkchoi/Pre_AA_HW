<h1>Add a new band</h1>

<form action="<%= bands_url %>" method="post" class="ui form">
  <%= auth_token %>
  <%= patch_input %>

  <label for="band_name">Band name</label>
  <input type="text" name="band[name]" id="band_name">
  <br><br>
  <input type="submit" class="ui button" value="Add band">
</form>
